{
    "model": "RandomForestClassifier",
    "arguments": "default",
    "type": "classification",
    "algorithm": "RandomForest",
    "dataset_props": {
        "type": "csv",
        "split": {
            "test_size": 0.2,
            "shuffle": true
        },
        "preprocess": {
            "missing_values": "mean",
            "encoding": {
                "type": "oneHotEncoding",
                "column": "Sex"
            },
            "scale": {
                "method": "standard",
                "target": "inputs"
            }
        }
    },
    "model_props": {
        "type": "classification",
        "algorithm": "RandomForest",
        "hyperparameter_search": {
            "method": "random_search",
            "scoring": "‘roc_auc’",
            "parameter_grid": {
                "max_depth": [
                    6,
                    10
                ],
                "n_estimators": [
                    100,
                    300
                ],
                "max_features": [
                    "auto",
                    "sqrt"
                ]
            },
            "arguments": {
                "cv": 5,
                "refit": true,
                "return_train_score": false,
                "verbose": 0
            }
        }
    },
    "data_path": "./examples/train_titanic.csv",
    "train_data_shape": [
        712,
        12
    ],
    "test_data_shape": [
        179,
        12
    ],
    "train_data_size": 712,
    "test_data_size": 179,
    "results_path": "./examples/fit/",
    "model_path": "./examples/fit/model.sav",
    "target": [
        "Survived"
    ],
    "results_on_test_data": {
        "accuracy_score": 0.8491620111731844,
        "f1_score": 0.7969924812030075,
